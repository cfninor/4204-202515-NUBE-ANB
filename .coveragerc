[run]
relative_files = True
source =
    anbapi/app
omit =
    */__init__.py
    anbapi/app/config.py
    anbapi/app/logging_config.py
    anbapi/app/prestart.py

[report]
omit =
    anbapi/app/tests/*
    **/__init__.py
    anbapi/app/config.py
    anbapi/app/logging_config.py
    anbapi/app/prestart.py
